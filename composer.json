{
    "name": "elvispt/zeteticelench",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "algolia/scout-extended": "^1.9",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.3",
        "laravel/scout": "^8.0",
        "laravel/telescope": "^3.2",
        "laravel/tinker": "^2.1",
        "laravel/ui": "^2.0",
        "league/commonmark": "^1.4",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-db-snapshots": "^1.6",
        "spatie/laravel-view-models": "^1.3",
        "thepinecode/i18n": "^0.9.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "permissions": [
            "chown -R www-data storage/framework/ && chown -R www-data storage/logs/ && chown -R www-data bootstrap/cache/ && chown -R www-data storage/app/laravel-db-snapshots"
        ],
        "tests": [
          "rm -f database/database.sqlite",
          "touch database/database.sqlite",
          "./vendor/bin/phpunit"
        ],
        "tests-coverage": [
          "rm -f database/database.sqlite",
          "touch database/database.sqlite",
          "./vendor/bin/phpunit --testdox --coverage-html tests/coverage"
        ]
    }
}
